// Solution using Recursion
class Solution {
    public static int robFrom(int[] nums , int i){
        if (i >= nums.length){
            return 0;
        }
        int include = nums[i] + robFrom( nums , i + 2);
        int exclude = robFrom( i + 1);
        return Math.max(inculde , exclude);
    }
    public int rob(int[] nums) {
        return robFrom(nums , 0);
       }
