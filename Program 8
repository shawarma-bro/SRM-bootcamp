class Solution {
    public static int minSquare(int n){
        if(n == 0){
            return 0;
        }
        int min = Integer.MAX_VALUE;
        for( int i = 1 ; i*i <= n ; i++ ){
            min = Math.min(min ,1 + minSquare(n - i*i));

        }
        return min;
    }
    public int numSquares(int n) {
        return minSquare(n);
    
        
    }
}
